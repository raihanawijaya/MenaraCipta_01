                            android:textSize="@dimen/_6sdp"
                            android:layout_width="@dimen/_40sdp"
                            android:layout_height="wrap_content"
                            android:text="Pilih "/>

                    </LinearLayout>

             </LinearLayout>
        </LinearLayout>



       </LinearLayout>



